{
  "name": "menucard-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "A multi-theme, completely accessible dashboard with menu card and components.",
  "dependencies": {
    "@iconify/react": "^5.0.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.8",
    "chart.js": "^4.4.9",
    "iconify": "^1.4.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router": "^7.0.2",
    "react-router-dom": "^7.0.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.9.0",
    "tailwindcss": "^4.1.8",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "autoprefixer": "^9.8.0",
    "color": "3.1.2",
    "commitizen": "4.1.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss-cli": "7.1.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "release-it": "^19.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind": "tailwindcss build public/assets/css/tailwind.css -o public/assets/css/tailwind.output.css",
    "build-prod": "env NODE_ENV=production postcss public/assets/css/tailwind.css -o public/assets/css/tailwind.output.css",
    "cz": "git-cz",
    "release": "release-it"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "tailwind",
    "windmill",
    "dashboard",
    "template",
    "admin",
    "menu",
    "react"
  ],
  "author": "Estevan Maito <ejmaito@gmail.com>",
  "license": "MIT"
}
